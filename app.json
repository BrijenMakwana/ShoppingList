{
  "expo": {
    "name": "Shopping Cart",
    "slug": "ShoppingCart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D8E9A8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#D8E9A8"
      },
      "permissions" : [],
      "package": "com.brijenmakwana.ShoppingCart"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
